            start_suspended true
        }
        pane command="uv" cwd="{{ .chezmoi.homeDir }}/programming/lehrplanPlus-Visualizer" size="50%" {
            args "run" "--group" "api" "granian" "--interface" "asgi" "api.main:app" "--reload"
