layout {
    cwd "{{ .chezmoi.homeDir }}"
    tab name="provisioning" hide_floating_panes=true {
        pane size=1 borderless=true {
            plugin location="zellij:tab-bar"
        }
        pane command="pi" cwd="{{ .chezmoi.homeDir }}/ops/provisioning" size="50%" {
            start_suspended true
        }
        pane command="/usr/lib/electron37/electron" cwd="{{ .chezmoi.homeDir }}/ops/provisioning" size="50%" {
            args "/usr/lib/obsidian/app.asar"
            start_suspended true
        }
        pane size=1 borderless=true {
            plugin location="zellij:status-bar"
        }
    }
    tab name="pi-societyChatGPT1" hide_floating_panes=true {
        pane size=1 borderless=true {
            plugin location="zellij:tab-bar"
        }
        pane command="pi" name="main" cwd="{{ .chezmoi.homeDir }}/pi-societyChatGPT1" size="70%" {
            start_suspended true
        }
        pane size="30%" split_direction="vertical" {
            pane command="node" name="web dev" cwd="{{ .chezmoi.homeDir }}/pi-societyChatGPT1/apps/web" size="25%" {
                args "{{ .chezmoi.homeDir }}/pi-societyChatGPT1/node_modules/.bin/next" "dev"
                start_suspended true
            }
            pane command="just" cwd="{{ .chezmoi.homeDir }}/pi-societyChatGPT1/apps/web" size="75%" {
                args "convex"
                start_suspended true
            }
        }
        pane size=1 borderless=true {
            plugin location="zellij:status-bar"
        }
    }
    tab name="lehrplanPLUS" hide_floating_panes=true {
